#include<bits/stdc++.h>
using namespace std;

int main(){
	int t;cin>>t;
	while(t--){
		int n; cin>>n;
		vector<int> a[10001]={};
		int ok=0;
		int root;
		while(n--){
			int u,v;
			char x;
			cin>>u>>v>>x;
			if(ok==0){
				root=u;
				ok=1;
			}
			if(x=='L'){
				if(a[u].size()>0){
					int e=a[u][0];
					a[u].pop_back();
					a[u].push_back(v);
					a[u].push_back(e);
				}else{
					a[u].push_back(v);
				}
			}else{
				a[u].push_back(v);
			}
		}
		queue<int> q;
		q.push(root);
		cout<<root<<" ";
		int an=1;
		while(q.empty()==0){
			int na=q.front();
			an++;
			if(an%2==0){
				for(int i=0;i<a[na].size();i++){
					cout<<a[na][i]<<" ";
					q.push(a[na][i]);
				}		
			}else{
				for(int i=a[na].size()-1;i>=0;i--){
					cout<<a[na][i]<<" ";
					q.push(a[na][i]);
				}			
			}
			q.pop();
		}
		cout<<endl;
	}
}
