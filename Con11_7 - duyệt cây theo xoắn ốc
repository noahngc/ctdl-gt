#include<bits/stdc++.h>
using namespace std;

int main(){
	int t;cin>>t;
	while(t--){
		int n; cin>>n;
		vector<int> a[10001]={};
		int ok=0;
		int root;
		while(n--){
			int u,v;
			char x;
			cin>>u>>v>>x;
			if(ok==0){
				root=u;
				ok=1;
			}
			if(x=='L'){
				if(a[u].size()>0){
					int e=a[u][0];
					a[u].pop_back();
					a[u].push_back(v);
					a[u].push_back(e);
				}else{
					a[u].push_back(v);
				}
			}else{
				a[u].push_back(v);
			}
		}
		queue<int> q;
		stack<int> s;
		q.push(root);
		cout<<root<<" ";
		while(q.empty()==0){
			while(q.empty()==0){
				int r=q.front();
				q.pop();
				for(int k:a[r]){
					cout<<k<<" ";
					s.push(k);
				}
			}
			stack<int> st;
			while(s.empty()==0){
				int r=s.top();
				s.pop();
				for(int k=a[r].size()-1;k>=0;k--){
					cout<<a[r][k]<<" ";
					st.push(a[r][k]);
				}
			}
			while(st.empty()==0){
				q.push(st.top());
				st.pop();
			}
		}
		cout<<endl;
	}
}
